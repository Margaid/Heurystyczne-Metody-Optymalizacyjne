             RowBox[{"y", "^", "2"}]}], ")"}]}]]}], "]"}]}], "-", 
      RowBox[{"Exp", "[", 
       RowBox[{
        RowBox[{"1", "/", "2"}], "*", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Cos", "[", 
           RowBox[{"2", "*", "Pi", "*", "x"}], "]"}], "+", 
          RowBox[{"Cos", "[", 
           RowBox[{"2", "*", "Pi", "*", "y"}], "]"}]}], ")"}]}], "]"}], "+", 
      "20", "+", "\[ExponentialE]"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
